{
  "name": "codingfuture-cflogsink",
  "version": "0.12.0",
  "author": "Andrey Galkin",
  "summary": "Centralized logging",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cflogsink",
  "project_page": "https://github.com/codingfuture/puppet-cflogsink",
  "issues_url": "https://github.com/codingfuture/puppet-cflogsink/issues",
