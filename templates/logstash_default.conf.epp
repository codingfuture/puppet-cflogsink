input {
    relp {
        id => "plain_relp"
        port => "${CF_PORT}"
    }
    relp {
        id => "secure_relp"
        port => "${CF_SECURE_PORT}"
        ssl_enable => true
        ssl_cacert => "${CF_TLS_CACERT}"
        ssl_cert => "${CF_TLS_CERT}"
        ssl_key => "${CF_TLS_KEY}"
        ssl_verify => true
    }
}

filter {
}

output {
    elasticsearch {
        id => "default_sink"
        hosts => "http://${DB_HOST}:${DB_PORT}"
    }
}
